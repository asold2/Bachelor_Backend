      <package pattern="*"></package>
    </packageSource>
  </packageSourceMapping>
  <packageSourceCredentials>
    <gitlab>
      <add key="Username" value="%NUGET_CI_USERNAME%" />
      <add key="ClearTextPassword" value="%CI_JOB_TOKEN%" />
    </gitlab>
  </packageSourceCredentials>
